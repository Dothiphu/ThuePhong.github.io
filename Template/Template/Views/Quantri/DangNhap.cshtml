
@{
    ViewBag.Title = "Đăng nhập";
    Layout = "~/Views/LayoutAdmin/Admin.cshtml";
}

<h2>Đăng nhập</h2>
<h1>@ViewBag.ThongBao</h1>
@using (Html.BeginForm())
{
    <span>Tài khoản:</span><br />
    @Html.TextBox("txtTaikhoan");
    <br />
    <span>Mật khẩu: </span><br />
    <input name="txtMatkhau" type="password" />
    <input type="Submit" id="Submit" name="btnSubmit" value="Gửi" />
    <br />
}

